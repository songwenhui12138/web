.navChart
  margin-top 8px
  width 100%
  height 36px
  & ul
    width 100%
    height 100%
    border-radius: 18px;
    border: solid 2px #ffffff;
    & li
      float left
      height 100%
      font-size: 14px;
      line-height 30px
      text-align center
      border-radius: 18px;
      cursor pointer
      &:hover
        color #1fdfe9
    & .active
      background-color rgba(0, 255, 255, 0.5)


