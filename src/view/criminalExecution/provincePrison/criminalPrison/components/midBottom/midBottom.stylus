.midBottom
  overflow hidden
  width 100%
  height 280px
  background url("../../../../../../assets/criminalExecution/midBot.png") no-repeat
  background-size 100% 100%
  .midContent
    width calc(100% - 26px)
    height calc(100% - 48px)
    padding-bottom 8px
    overflow auto
  .sdcmScrollbar:hover::-webkit-scrollbar
    display block
  .sdcmScrollbar::-webkit-scrollbar
    display none
  .sdcmScrollbar::-webkit-scrollbar-track
    border: none;
  .content
    width 100%
    height 100%
