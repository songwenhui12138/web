.prisonNav
  float left
  width 40px;
  height 100%
  background url("../../../../../../assets/criminalExecution/frameNav.png") no-repeat
  background-size 100% 100%
  padding 16px 6px 0
  cursor pointer
  & i
    font-size 16px
    text-align center
  & span
    margin-top 30px
    font-size: 24px;
    display inline-block
    text-align center
  .drawer
    position fixed
    width 100%
    height 100%
    top 0
    left 0
